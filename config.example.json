{
  "agents": {
    "defaults": {
      "workspace": "~/.nanobot/workspace",
      "model": "anthropic/claude-opus-4-5",
      "max_tokens": 8192,
      "temperature": 0.7,
      "max_tool_iterations": 20
    }
  },
  "channels": {
    "mobile": {
      "enabled": true,
      "websocket_port": 18791,
      "tls_enabled": false,
      "tls_cert_path": null,
      "tls_key_path": null,
      "max_connections": 100,
      "heartbeat_interval": 30
    }
  },
  "providers": {
    "anthropic": {
      "api_key": "",
      "api_base": null,
      "extra_headers": null
    },
    "openai": {
      "api_key": "",
      "api_base": null,
      "extra_headers": null
    },
    "openrouter": {
      "api_key": "REPLACE_WITH_YOUR_OPENROUTER_API_KEY",
      "api_base": null,
      "extra_headers": null
    },
    "deepseek": {
      "api_key": "",
      "api_base": null,
      "extra_headers": null
    },
    "groq": {
      "api_key": "",
      "api_base": null,
      "extra_headers": null
    },
    "zhipu": {
      "api_key": "",
      "api_base": null,
      "extra_headers": null
    },
    "dashscope": {
      "api_key": "",
      "api_base": null,
      "extra_headers": null
    },
    "vllm": {
      "api_key": "",
      "api_base": null,
      "extra_headers": null
    },
    "gemini": {
      "api_key": "REPLACE_WITH_YOUR_GEMINI_API_KEY",
      "api_base": null,
      "extra_headers": null
    },
    "moonshot": {
      "api_key": "",
      "api_base": null,
      "extra_headers": null
    },
    "aihubmix": {
      "api_key": "",
      "api_base": null,
      "extra_headers": null
    }
  },
  "gateway": {
    "host": "0.0.0.0",
    "port": 18790
  },
  "tools": {
    "web": {
      "search": {
        "api_key": "",
        "max_results": 5
      }
    },
    "exec": {
      "timeout": 60
    },
    "restrict_to_workspace": false
  },
  "auth": {
    "jwt_secret": "REPLACE_WITH_STRONG_SECRET_MIN_32_CHARS",
    "jwt_algorithm": "HS256",
    "jwt_expiry_hours": 720,
    "pairing_session_expiry_minutes": 5,
    "oauth_enabled": false,
    "oauth_providers": {}
  },
  "enterprise": {
    "organization_name": "My Organization",
    "rate_limit_enabled": true,
    "rate_limit_requests_per_minute": 60,
    "audit_log_enabled": true,
    "audit_log_path": "~/.nanobot/logs/audit.log",
    "ip_whitelist_enabled": false,
    "ip_whitelist": []
  },
  "network": {
    "proxy_enabled": false,
    "proxy_url": null,
    "vpn_required": false,
    "allowed_origins": ["*"]
  }
}
